#!/bin/bash
echo "Let's get it started"
g++ Ex_1_bad.cpp -o 1
g++ Ex_1_good.cpp -o 2
g++ Ex_2_bad.cpp -o 3
g++ Ex_2_good.cpp -o 4
g++ Ex_3_bad.cpp -o 5
g++ Ex_3_good.cpp -o 6
echo "Go!"
#Вставка в конец мапы без улучшений (Ex_1_bad.cpp)
(time ./1) 2> times.txt
echo "1"
#Вставка в конец мапы c хинтом (Ex_1_good.cpp)
(time ./2) 2>> times.txt
echo "2"
#Программа генерит псевдослучайное число, если число есть в мапе в качестве ключа, то ничего не делать, иначе посчитать, количество ключей, меньших сгенерированого числа и добавить в мапу элемент вида {сгенерированое число: число ключей, меньших числа}. Без оптимизации (Ex_2_bad.cpp)
(time ./3) 2>> times.txt
echo "3"
#Программа генерит псевдослучайное число, если число есть в мапе в качестве ключа, то ничего не делать, иначе посчитать, количество ключей, меньших сгенерированого числа и добавить в мапу элемент вида {сгенерированое число: число ключей, меньших числа}. С хинтом (Ex_2_good.cpp)
(time ./4) 2>> times.txt
echo "4"
#Вставка ключей 0, 2, 4, 6..., 1, 3, 5... без оптимизации (Ex_3_bad.cpp)
(time ./5) 2>> times.txt
echo "5"
#Вставка ключей 0, 2, 4, 6..., 1, 3, 5... с хинтом (Ex_3_good.cpp)
(time ./6) 2>> times.txt
echo "6"

